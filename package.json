{
  "name": "teststore",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build:api": "nx build api",
    "build:teststore": "nx build teststore",
    "test": "nx run-many --all --target=test",
    "test:ci": "nx run-many --all --target=test --skip-nx-cache",
    "lint": "nx workspace-lint && nx run-many --target=lint --all=true --verbose",
    "e2e": "nx e2e teststore-e2e",
    "e2e:ci": "nx run-many --target=e2e --all --maxParallel=5 --browser chrome",
    "e2e:open": "nx e2e teststore-e2e --watch",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "seed": "nx run seeder:build && node ./dist/apps/seeder/main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "8.0.3",
    "@nrwl/angular": "14.1.5",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "typeorm": "0.2.45",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.5",
    "@nrwl/cli": "14.1.5",
    "@nrwl/cypress": "14.1.5",
    "@nrwl/eslint-plugin-nx": "14.1.5",
    "@nrwl/jest": "14.1.5",
    "@nrwl/linter": "14.1.5",
    "@nrwl/nest": "14.1.5",
    "@nrwl/node": "14.1.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.1.5",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.26.0",
